# Строка подключения к PostgreSQL
# На Railway подставляется автоматически через переменную DATABASE_URL
DATABASE_URL="postgresql://user:password@localhost:5432/livebot?schema=public"

# Секрет для JWT токенов (обязательно замените на свой на проде!)
JWT_SECRET="your-secret-key-here"

# Порт сервера (Railway подставляет автоматически)
PORT=3001
